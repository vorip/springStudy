<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.kr.spring.srv.mapper.BbsMapper">
	<select id="getBbs" resultType="BbsVo">
		SELECT
			SEQ AS seq
			, TITLE AS title
			, CONTENTS AS contents
			, WRITER AS writer
			, REGDATE AS regDate
		FROM
			BBS
	</select>
</mapper>